{
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "importOrder": [
    "^(util|path|fs|http|http2|crypto|url|os|reflect-metadata)$",
    "<THIRD_PARTY_MODULES>",
    "^\\.\\.",
    "^[./]/",
    "^\\.$"
  ],
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true
}
